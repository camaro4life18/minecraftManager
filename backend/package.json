{
  "name": "minecraft-server-manager-backend",
  "version": "1.0.0",
  "description": "Minecraft server management API for Proxmox",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:backend": "node --watch server.js",
    "init-db": "node init-db.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "syntax-check": "node --check server.js && node --check sshClient.js && node --check database.js && node --check auth.js && node --check middleware.js && node --check swagger.js && echo \"âœ“ All files syntax check passed\"",
    "pretest": "npm run syntax-check"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "pg": "^8.11.3",
    "ssh2": "^1.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
